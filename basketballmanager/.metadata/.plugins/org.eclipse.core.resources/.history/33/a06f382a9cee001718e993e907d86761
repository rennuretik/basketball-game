	controlPanelDefense()
	{
		this.setSize(Const_UI.CONTROLDETAIL_WIDTH, Const_UI.CONTROLDETAIL_HEIGHT);
		this.setBackground(Color.YELLOW);

		this.setVisible(false);
	}
}
