package Basketballmanager.player;

public class player {
	//attribute measure the condition of player
	public final String name;
	public final int speed;
	public final int strength;
	public final int score;
	public final int assist;
	public final int rebound;
	public final int enthusiam;
	public final int defense;
	public final int offense;
	public final int block;
	public final int steal;
	public final int turnover;
	final int focus;
	final int shooter;
	final int max_energy;
	
}
